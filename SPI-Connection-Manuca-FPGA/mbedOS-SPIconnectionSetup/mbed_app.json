{
    "config": {
        "main-stack-size": {
            "help": "Specifies stack size of the main thread",
            "value": 8192
        }
    },
    "requires": ["bare-metal"]
    ,"macros": [
        "LINUX"
    ],
    "target_overrides": {
        "*": {
            "target.printf_lib"                         : "std",
            "target.stdio_uart_tx"            : "PC_12",
	    "target.stdio_uart_rx"            :  "PD_2",
	    "platform.stdio-baud-rate": 9600
        }
    }
}
