{
    "config": {
        "main-stack-size": {
            "help": "Specifies stack size of the main thread",
            "value": 65536
        }
    },
    "requires": ["bare-metal"]
    ,"macros": [
        "LINUX"
    ],
    "target_overrides": {
        "*": {
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-buffered-serial": 1,
            "target.printf_lib"                         : "std"
        }
    }
}
